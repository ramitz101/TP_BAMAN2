<Window x:Class="Barman.ViewAutreDossier.FenetreOptionsImprimer"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Barman.ViewAutreDossier"
        mc:Ignorable="d"
        Title="Option du téléchargement" Height="191.892" Width="275.676"
        WindowStartupLocation="CenterOwner">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>

        <Button Name="btnSeulementOuvrir" Content="Ouvrir le PDF" Height="auto" Grid.Row="0" Margin="5,5,5,5" Click="btnSeulementOuvrir_Click"  />
        <Button Name="btnSeulementSauvegarder" Content="Sauvegarder le PDF" Height="auto" Grid.Row="1" Margin="5,5,5,5" Click="btnSeulementSauvegarder_Click" />
        <Button Name="btnSauvegarderEtOuvrir" Content="Sauvegarder et ouvrir le PDF" Height="auto" Grid.Row="2" Margin="5,5,5,5" Click="btnSauvegarderEtOuvrir_Click"  />
    </Grid>
</Window>
